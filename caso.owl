    </owl:Class>
    


    <!-- http://david.bellingroth.org/caso.owl#Smartphone -->

    <owl:Class rdf:about="http://david.bellingroth.org/caso.owl#Smartphone">
        <rdfs:subClassOf rdf:resource="&securityontology;Asset"/>
        <rdfs:subClassOf>
            <owl:Restriction>
