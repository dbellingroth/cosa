        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&securityontology;has_vulnerability"/>
                <owl:hasValue rdf:resource="http://david.bellingroth.org/caso.owl#visibility_of_content"/>
            </owl:Restriction>
